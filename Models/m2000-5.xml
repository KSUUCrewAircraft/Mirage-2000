  <nasal>
    <load>
      # WARNING : The on-load Nasal is not executed when this file is loaded as the
      # user aircraft.
      #
      # load the needed modules if it hasn't been done yet.
      # load handles broadcast of:
      #   - external load set.
      if(! contains(globals, "m2000_load"))
      {
          var file = call(func getprop("/sim/fg-aircraft") ~ "/Mirage-2000/Nasal/ext_stores.nas", nil, var err = []);
          var code = call(func compile(io.readfile(file), file), nil, err);
          print("Path 1. Error : " ~size(err));
          if(size(err) == 0)
          {
              io.load_nasal(file, "m2000_load");
          }
      }
      if(! contains(globals, "m2000_load"))
      {
          var file = call(func getprop("/sim/fg-aircraft[1]") ~ "/Mirage-2000/Nasal/ext_stores.nas", nil, var err = []);
          var code = call(func compile(io.readfile(file), file), nil, err);
          print("Path 2. Error : " ~size(err));
          if(size(err) == 0)
          {
              io.load_nasal(file, "m2000_load");
          }
      }
      if(! contains(globals, "m2000_load"))
      {
          var file = call(func getprop("/sim/fg-root") ~ "/Aircraft/Mirage-2000/Nasal/ext_stores.nas", nil, var err = []);
          var code = call(func compile(io.readfile(file), file), nil, err);
          print("Path 3. Error : " ~ size(err));
          if(size(err) == 0)
          {
              io.load_nasal(file, "m2000_load");
          }
      }
      var update = aircraft.overlay_update.new();
      update.add("Aircraft/Mirage-2000/Models/Liveries", "sim/model/livery/file");
      update.add("Aircraft/Mirage-2000/Models/Logos/", "sim/multiplay/generic/string");
      
      #var livery_update = aircraft.livery_update.new("Aircraft/Mirage-2000/Models/Liveries", 10);
      #var update = aircraft.overlay_update.new();
      #update.add("Aircraft/F4U/Models/Logos", "sim/multiplay/generic/string");
      
      var self = cmdarg();
      var float_0 = self.getNode("sim/multiplay/generic/float[0]", 1);
      var float_1 = self.getNode("sim/multiplay/generic/float[1]", 1);
      
      var myLoadString = self.getNode("sim/multiplay/generic/string[1]",1);
      
      var crew_door = self.getNode("sim/model/door-positions/crew/position-norm", 1);
      var passenger_door = self.getNode("sim/model/door-positions/passenger/position-norm", 1);
      
      # Update Time
      #print("Creating the Decodeur");
      var myDecodeur = m2000_load.Decode_Load.new(self,myLoadString,0.5);
      myDecodeur.decode();
      
      crew_door.alias(float_0);
      passenger_door.alias(float_1);
      #print("multiplay");
      #MP.loads(1);
    </load>
    <unload>
      myDecodeur.stop();
      #livery_update.stop();
      update.stop();
      crew_door.unalias();
      passenger_door.unalias();
      #MP.loads(0);
    </unload>
  </nasal>

