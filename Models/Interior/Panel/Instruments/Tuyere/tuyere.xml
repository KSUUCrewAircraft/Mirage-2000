<?xml version="1.0" encoding="iso-8859-1"?>

<PropertyList>

  <path>tuyere.ac</path>

  <animation>
    <object-name>base</object-name>
    <object-name>visses</object-name>
    <object-name>face</object-name>
    <object-name>needle</object-name>
    <object-name>num1-1</object-name>
    <object-name>num1-2</object-name>
    <object-name>num1-3</object-name>
    <object-name>num2-0</object-name>
    <object-name>num2-1</object-name>
    <object-name>num2-2</object-name>
    <object-name>num2-3</object-name>
    <object-name>num3-0</object-name>
    <object-name>num3-1</object-name>
    <object-name>num3-2</object-name>
    <object-name>num3-3</object-name>
  </animation>

  <animation>
    <type>material</type>
    <object-name>base</object-name>
    <object-name>visses</object-name>
    <condition>
      <not>
        <property>sim/rendering/rembrandt/enabled</property>
      </not>
    </condition>
    <emission>
      <red>   0.13 </red>
      <green> 0.13 </green>
      <blue>  0.13 </blue>
    </emission>
  </animation>

  <animation>
    <type>material</type>
    <object-name>base</object-name>
    <object-name>visses</object-name>
    <condition>
      <property>sim/rendering/rembrandt/enabled</property>
    </condition>
    <emission>
      <red>   0.05 </red>
      <green> 0.05 </green>
      <blue>  0.05 </blue>
    </emission>
  </animation>

  <animation>
    <type>material</type>
    <object-name>face</object-name>
    <object-name>needle</object-name>
    <object-name>num1-1</object-name>
    <object-name>num1-2</object-name>
    <object-name>num1-3</object-name>
    <object-name>num2-0</object-name>
    <object-name>num2-1</object-name>
    <object-name>num2-2</object-name>
    <object-name>num2-3</object-name>
    <object-name>num3-0</object-name>
    <object-name>num3-1</object-name>
    <object-name>num3-2</object-name>
    <object-name>num3-3</object-name>
    <emission>
      <red>   0.50 </red>
      <green> 0.25 </green>
      <blue>  0.25 </blue>
      <factor-prop>/systems/electrical/outputs/instrument-lights</factor-prop>
    </emission>
  </animation>

  <!-- ________________ n1 % _________________-->
  <animation>
    <type>textranslate</type>
    <object-name>num3-0</object-name>
    <property>/engines/engine/n1</property>
    <factor>0.001</factor>
    <step>100</step>
    <axis>
      <x>0</x>
      <y>1</y>
      <z>0</z>
    </axis>
  </animation>

  <animation>
    <type>textranslate</type>
    <object-name>num3-1</object-name>
    <property>/engines/engine/n1</property>
    <factor>0.01</factor>
    <step>10</step>
    <axis>
      <x>0</x>
      <y>1</y>
      <z>0</z>
    </axis>
  </animation>

  <animation>
    <type>textranslate</type>
    <object-name>num3-2</object-name>
    <property>/engines/engine/n1</property>
    <factor>0.1</factor>
    <step>1</step>
    <axis>
      <x>0</x>
      <y>1</y>
      <z>0</z>
    </axis>
  </animation>

  <animation>
    <type>textranslate</type>
    <object-name>num3-3</object-name>
    <property>/engines/engine/n1</property>
    <factor>1</factor>
    <step>0.1</step>
    <axis>
      <x>0</x>
      <y>1</y>
      <z>0</z>
    </axis>
  </animation>

  <!-- _______________________EGT C°_____________________-->
  <animation>
    <type>textranslate</type>
    <object-name>num2-0</object-name>
    <property>/engines/engine[0]/egt-degC</property>
    <factor>0.0001</factor>
    <step>1000</step>
    <axis>
      <x>0</x>
      <y>1</y>
      <z>0</z>
    </axis>
  </animation>

  <animation>
    <type>textranslate</type>
    <object-name>num2-1</object-name>
    <property>/engines/engine[0]/egt-degC</property>
    <factor>0.001</factor>
    <step>100</step>
    <axis>
      <x>0</x>
      <y>1</y>
      <z>0</z>
    </axis>
  </animation>

  <animation>
    <type>textranslate</type>
    <object-name>num2-2</object-name>
    <property>engines/engine[0]/egt-degC</property>
    <factor>0.01</factor>
    <step>10</step>
    <axis>
      <x>0</x>
      <y>1</y>
      <z>0</z>
    </axis>
  </animation>

  <animation>
    <type>textranslate</type>
    <object-name>num2-3</object-name>
    <property>engines/engine[0]/egt-degC</property>
    <factor>0.1</factor>
    <step>1</step>
    <axis>
      <x>0</x>
      <y>1</y>
      <z>0</z>
    </axis>
  </animation>

  <!-- _______________________EPR  Engine pressure ratio_____________________-->
  <animation>
    <type>textranslate</type>
    <object-name>num1-1</object-name>
    <property>/instrumentation/consumables/consumption_per_min</property>
    <factor>0.001</factor>
    <step>100</step>
    <axis>
      <x>0</x>
      <y>1</y>
      <z>0</z>
    </axis>
  </animation>

  <animation>
    <type>textranslate</type>
    <object-name>num1-2</object-name>
    <property>/instrumentation/consumables/consumption_per_min</property>
    <factor>0.01</factor>
    <step>10</step>
    <axis>
      <x>0</x>
      <y>1</y>
      <z>0</z>
    </axis>
  </animation>

  <animation>
    <type>textranslate</type>
    <object-name>num1-3</object-name>
    <property>/instrumentation/consumables/consumption_per_min</property>
    <factor>1</factor>
    <step>0.1</step>
    <axis>
      <x>0</x>
      <y>1</y>
      <z>0</z>
    </axis>
  </animation>

  <animation>
    <type>textranslate</type>
    <object-name>needle</object-name>
    <property>/controls/engines/engine/n1</property>
    <factor>0.0049</factor>
    <step>0.001</step>
    <axis>
      <x>0</x>
      <y>1</y>
    </axis>
  </animation>

</PropertyList>
